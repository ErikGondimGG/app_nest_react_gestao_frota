model veiculos {
  id               Int       @id @default(autoincrement())
  placa            String?
  marca            String?
  modelo           String?
  ano_fabricacao   Int?
  ano_modelo       Int?
  cor              String?
  tag              String?
  km               Int?
  hr               Int?
  capacidade_carga Int?
  tipo             String?
  created_at       DateTime  @default(now())
  updated_at       DateTime  @default(now())
  deleted_at       DateTime?

  @@map("veiculos")
  @@schema("Base api_bot_wpp")
}
